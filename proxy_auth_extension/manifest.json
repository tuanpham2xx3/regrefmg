{
    "version": "1.0.0",
    "manifest_version": 2,
    "name": "Proxy Auth Handler",
    "description": "Automatically handles HTTP proxy authentication",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    }
}
